<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.gghangura.adminconsole.SoapUserDetail">

    <com.good.gd.widget.GDTextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="75dp"
        android:layout_marginStart="75dp"
        android:layout_marginTop="50dp"
        android:text="Display Name"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.good.gd.widget.GDTextView
        android:id="@+id/dNameText"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="50dp"
        android:text="No Display Name"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.good.gd.widget.GDTextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Login Name"
        app:layout_constraintLeft_toLeftOf="@+id/textView2"
        app:layout_constraintRight_toRightOf="@+id/textView2"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <com.good.gd.widget.GDTextView
        android:id="@+id/lNameText"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="No Login Name"
        app:layout_constraintLeft_toLeftOf="@+id/dNameText"
        app:layout_constraintRight_toRightOf="@+id/dNameText"
        app:layout_constraintTop_toBottomOf="@+id/dNameText" />

    <com.good.gd.widget.GDTextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="Email"
        app:layout_constraintLeft_toLeftOf="@+id/textView4"
        app:layout_constraintRight_toRightOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <com.good.gd.widget.GDTextView
        android:id="@+id/eMailText"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:text="No Email"
        app:layout_constraintLeft_toLeftOf="@+id/lNameText"
        app:layout_constraintRight_toRightOf="@+id/lNameText"
        app:layout_constraintTop_toBottomOf="@+id/lNameText" />

    <ListView
        android:id="@+id/deviceListView"
        android:layout_width="316dp"
        android:layout_height="209dp"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Button
        android:id="@+id/applicationButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:text="Get Applications"
        app:layout_constraintBottom_toTopOf="@+id/deviceListView"
        app:layout_constraintTop_toBottomOf="@+id/eMailText"
        app:layout_constraintVertical_bias="0.571"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
